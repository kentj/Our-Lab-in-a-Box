http://remote.tlsv-lab.nccoe.test {
  file_server
}

guacamole.tlsv-lab.nccoe.test {
  tls /certs/guacamole/guacamole.tlsv-lab.nccoe.test.crt /certs/guacamole/guacamole.tlsv-lab.nccoe.test.key

  handle /guacamole/* {
    reverse_proxy tlsv-lab-guacamole:8080
  }  
}

keycloak.tlsv-lab.nccoe.test {
    tls /certs/keycloak/keycloak.tlsv-lab.nccoe.test.crt /certs/keycloak/keycloak.tlsv-lab.nccoe.test.key
    reverse_proxy tlsv-lab-keycloak:80
}

http://php-ldap-admin.tlsv-lab.nccoe.test {
  reverse_proxy tlsv-lab-phpldapadmin:80
}

